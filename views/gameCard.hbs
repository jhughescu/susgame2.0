Game detail:
<table><tbody>
    {{#each game}}
    <tr><td>{{@key}}</td><td id='val_{{@key}}'>{{this}}</td></tr>
    {{/each}}
</tbody></table>
<br>
<div class='vertstack'>
    <div class='link{{#if gameInactive}} disabled{{/if}}' id='makePres'>Launch Presentation</div>
    <div class='link{{#if gameInactive}} disabled{{/if}}' id='makeFakes'>Launch Fake Generator</div>
    <div class='link{{#if gameInactive}} disabled{{/if}}' id='gameReset'>Reset game</div>
    <div class='link{{#if gameInactive}} disabled{{/if}}' id='gameEnd'>End game</div>
</div>
<div class='qrcode' id='qrcodegame'>
   game:
    <div class='colourStrip' style='background-color: {{game.idColour}}'></div>
    <img src='public/assets/qr/qrcode-{{game.uniqueID}}.svg'>
</div>
<div class='qrcode' id='qrcodepres'>
   presentation:
    <div class='colourStrip' style='background-color: {{game.idColour}}'></div>
    <img src='public/assets/qr/qrcode-{{game.uniqueID}}-presentation.svg'>
</div>
